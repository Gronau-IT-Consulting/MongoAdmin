    <script>

    var currentUsername = "test";

    function checkLogin() {

        mongoSitesApi.auth_check()
            .then(function (logged_in) {

                if(!logged_in) {

                    window.location.href = "/login";
                }

                console.log(logged_in);
            });
    }

    function loadUserName(cb) {

        mongoSitesApi.auth_check()
            .then(function (logged_in) {

                if(!logged_in) {

                    cb('');
                }
                else {

                    cb(logged_in._id);
                }
            });
    }

    function logout() {

        mongoSitesApi.auth_logout().then(function () {

                window.location.href = "/login";
            });
    }
    </script>
